<http://intelligentsystemproj1.io/data/courses/PHIL_612> a isp:Course ;
    dc:description " This course studies the texts central to the development of ancient philosophical thought, such as works by Plato and Aristotle. Note: Students who have received credit for PHIL 601 or PHIL 602 may not take this course for credit." ;
    dc:identifier 612 ;
    dc:subject "PHIL" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fasc/phil.html" ;
