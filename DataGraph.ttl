    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fasc/socianth-ma.html" ;
    foaf:name "Ethnographic Research and Ethics" .

<http://intelligentsystemproj1.io/data/courses/ANTH_620> a isp:Course ;
    dc:description " This course examines a range of methods and styles for presenting ethnographic material, from ethnographic realism to fiction, and encourages further experimentation." ;
    dc:identifier 620 ;
    dc:subject "ANTH" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fasc/socianth-ma.html" ;
    foaf:name "Writing Ethnography" .

<http://intelligentsystemproj1.io/data/courses/ANTH_630> a isp:Course ;
    dc:description " This course explores emergent concepts, methods and topics in anthropology. Content changes in accordance with the research focus of the professor leading the course." ;
    dc:identifier 630 ;
    dc:subject "ANTH" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fasc/socianth-ma.html" ;
