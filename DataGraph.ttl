    dc:identifier 623 ;
    dc:subject "ATRP" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fofa/cats-art.html" ;
    foaf:name "Advanced Art Therapy Practicum Supervision I (3 credits)" .

<http://intelligentsystemproj1.io/data/courses/ATRP_624> a isp:Course ;
