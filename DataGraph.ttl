    dc:description " A studio course providing opportunity for intense investigation into a wide range of approaches to painting. Under the supervision of a professor the structure of the course allows each student to pursue individual interests in painting." ;
    dc:identifier 610 ;
    dc:subject "PTNG" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fofa/star.html" ;
    foaf:name "Painting I (6 credits)" .

<http://intelligentsystemproj1.io/data/courses/PTNG_611> a isp:Course ;
    dc:description " A continuation of PTNG 610." ;
    dc:identifier 611 ;
    dc:subject "PTNG" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fofa/star.html" ;
    foaf:name "Painting II (6 credits)" .

<http://intelligentsystemproj1.io/data/courses/PTNG_612> a isp:Course ;
