    dc:identifier 693 ;
    dc:subject "ANTH" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fasc/socianth-ma.html" ;
    foaf:name "Essay Proposal" .

<http://intelligentsystemproj1.io/data/courses/ANTH_694> a isp:Course ;
    dc:description " Students spend two to three months reviewing the literature (which may include both academic and grey literature sources) on their proposed topic. The review forms the basis of the students' essay." ;
    dc:identifier 694 ;
    dc:subject "ANTH" ;
    rdfs:seeAlso "http://www.concordia.ca/academics/graduate/calendar/current/fasc/socianth-ma.html" ;
