@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://focu.io/schema#> .
@prefix focudata: <http://focu.io/data#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dbpedia: <http://dbpedia.org/ontology/university> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dbr: <http://dbpedia.org/resource/>  .

dbr:Universities rdf:type rdfs:Class ;
	owl:sameAs dbpedia:university ;
	rdfs:subClassOff dbpedia:universty ;
	foaf:name xsd:string;
	rdfs:label "University class" .
	
dbr:Courses rdf:type rdfs:Class ;
	rdfs:subClassOf rdf:University ;
	foaf:name xsd:string;
	dc:subject xsd:string ;
	dc:Identifier xsd:nonNegativeInteger;
	dc:desciption xsd:string ;
	rdfs:label "Courses class" .

dbr:topics rdfs:range rdfs:Class ;
	rdfs:isDefinedBy <http://dbpedia.org/ontology/university> ;
	rdfs:subClassOf rdf:course;
	rdfs:label "Topics in a course" .

ex:coursereport	rdf:type rdfs:Class ;
    ex:grade xsd:string ;
    rdfs:domain dbr:Courses  ;
	rdfs:label "Course report class" .
	
dbr:students rdf:type rdfs:Class ;
	rdfs:isDefinedBy dbpedia:student ;
	foaf:firstName xsd:string;
	foaf:lastName xsd:string;
	dc:Identifier xsd:nonNegativeInteger;
	foaf:mbox xsd:string ;
	rdfs:label "Students Class" .
