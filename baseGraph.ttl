@prefix dbp: <http://dbpedia.org/property/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix isp: <http://intelligentsystemproj1.io/schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dbr:Grading rdfs:label "CourseGrade"@en ;
    dbp:score xsd:string ;
    dc:subject dbr:Course ;
    rdfs:comment "Courses with Grades of a student"@en .

dc:coverage rdfs:label "coveredIn"@en ;
    rdfs:comment "Covered in University"@en ;
    rdfs:domain dbr:University ;
    rdfs:range dbr:Course .

dc:hasPart rdfs:label "PartOfCourse"@en ;
    rdfs:comment "Topics lying under Courses"@en ;
    rdfs:domain dbr:Course ;
    rdfs:range dbr:Concept .

foaf:member rdfs:label "MemberOf"@en ;
    rdfs:comment "Student member of University"@en ;
    rdfs:domain dbr:Student ;
    rdfs:range dbr:University .

foaf:topic_interest rdfs:label "CourseTaken"@en ;
    rdfs:comment "Courses taken by student"@en ;
    rdfs:domain dbr:Student ;
    rdfs:range dbr:Grade .

dbr:Concept rdfs:label "CourseTopic"@en ;
    dc:identifier xsd:int ;
    dc:source xsd:string ;
    dc:subject xsd:string ;
    rdfs:comment "Class for a Course Topic"@en ;
    foaf:name xsd:string .

dbr:Student rdfs:label "Student"@en ;
    dc:identifier xsd:int ;
    rdfs:comment "Class for a Student"@en ;
    foaf:familyName xsd:string ;
    foaf:givenName xsd:string ;
    foaf:mbox xsd:string .

dbr:University rdfs:label "University"@en ;
    dc:source xsd:string ;
    rdfs:comment "Class for a University"@en ;
    foaf:name xsd:string .

dbr:Course rdfs:label "Course"@en ;
    dc:description xsd:string ;
    dc:identifier xsd:int ;
    dc:subject xsd:string ;
    rdfs:comment "Class for a Course"@en ;
    rdfs:seeAlso xsd:string ;
    foaf:name xsd:string .

